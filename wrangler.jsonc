{
  "name": "farewell-unified",
  "main": "src/index.ts",
  "compatibility_date": "2025-06-18",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "assets": {
    "directory": "./public",
    "binding": "ASSETS"
  },
  "d1_databases": [
    {
      "binding": "FWHY_D1",
      "database_name": "farewell-db",
      "database_id": "76dc7b30-005a-4268-afa0-10b2fe242253"
    }
  ],
  "r2_buckets": [
    {
      "binding": "FWHY_IMAGES",
      "bucket_name": "fwhy-images"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "SESSIONS_KV",
      "id": "2038b95e785545af8486bc353c3cbe62",
      "preview_id": "2038b95e785545af8486bc353c3cbe62"
    },
    {
      "binding": "BLOG_KV",
      "id": "6ee9ab6b71634a4eb3e66de82d8dfcdc",
      "preview_id": "6ee9ab6b71634a4eb3e66de82d8dfcdc"
    }
  ],
  "vars": {
    "ADMIN_USERNAME": "anmid",
    "ENVIRONMENT": "production"
  },
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "tail_consumers": [
    {
      "service": "farewell-unified",
      "environment": "production"
    }
  ],
  "routes": [
    { "pattern": "dev.farewellcafe.com/*", "zone_name": "farewellcafe.com" },
    { "pattern": "admin.farewellcafe.com/*", "zone_name": "farewellcafe.com" }
  ]
}
